
public class Capsule {
	public static void main(String[]args){
		Person taro = new Person("山田太郎",20);
		System.out.println(taro.getName());
		taro.setName("花子");
		System.out.println(taro.getName());

		Person ensyu = new Person("演習5",30);
		ensyu.setName("演習5");
		ensyu.setAge("30");
		System.out.println(ensyu.getAge());


	}
}

